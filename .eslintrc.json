{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "jasmine"
    ],
    "rules": {
        "no-console": [0],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "no-underscore-dangle": [0]
    }
}
